mixin sProdCard(data)
	// start sProdCard
	+b.SECTION.sProdCard.section#sProdCard&attributes(attributes)
		+e.container.container
			+img('img/svg/square.svg').square.square--1
			+e.row.row
				+e.col.col-xxl-auto
					+e.swiper-wrap
						+e.slider--js.swiper
							.swiper-wrapper
								-for(let i = 0; i < 3; i++)
									+e.slide.swiper-slide
										+e.img-wrap
											.img-wrap-center
												+img('img/sProdCard-img-1.png')
						+arrow()
						+e.links
							+e.link(href="#")
								+icon('like')
							+e.link(href="#")
								+icon('diagram')
							+e.link(href="#")
								+icon('cart')
				.col-xxl
					.row
						.col
							+e.head-wrap
								+img('img/stars.png')
								+e.creator 
									| Производитель:
									+img('img/logo.png')
						.col-12
							+e.text-wrap
								p Габариты: ВxШxГ в мм / 1860x500x530
								p Тип: сборный
								p Количество человек: 1
					+e.main-row.row
						.col-md
							+e.headline
								h4 29 010 руб.
								+e.has Есть в наличии
							+e.products
								h6 Выберите товар:
								+e.radio-wrap
									-for(let i = 3; i<=8; i++)
										+in-c('radio', '', 'product')
											+e.count Количество полок: #[!= i]
											+e.articul Артикул: 21500
							+e.accessories
								h6 Аксессуары:
								-for(let i = 0; i<2; i++)
									+e.inner-row.row
										.col-12.col-sm
											+in-c('radio', 'ВЛ-К Панель облицовочная к опоре', 'accessory')
										.col-auto
											+e.articul 
												| Артикул: 21500
												+addblock()
										.col-auto
											+e.price Цена: 10 500 руб.
							+e.choose-colors
								h6 Выберите цвет:
								+e.colors-row.row
									.col-12.col-sm-auto
										+e.color-head ВЛ-К Панель облицовочная к опоре
									.col-auto
										+e.colors
											+in-c('radio', '', 'color')(style='--bg: #fedd51')
											+in-c('radio', '', 'color')(style='--bg: #fe7e37')
											+in-c('radio', '', 'color')(style='--bg: #ca1c18')
											+in-c('radio', '', 'color')(style='--bg: #80257f')
											+in-c('radio', '', 'color')(style='--bg: #346cb4')
											+in-c('radio', '', 'color')(style='--bg: #84c5be')
											+in-c('radio', '', 'color')(style='--bg: #31823c')
									.col-auto
										+addblock()
									.col-auto
										+e.price Цена: 10 500 руб.
							+e.footer-row.row
								.col-auto
									+e.btn.btn.btn-secondary В корзину
								.col-auto
									+addblock()
								.col-auto
									+e.buy(href="#") Купить в 1 клик
						.col-md-auto
							+e.head-row.row
								.col
									+e.btn-dicount.btn.btn-primary(href='#') Запросить скидку
								.col-auto
									+e.print(href='#')
										+icon('print')
							+e.delivery
								h6 Доставка
								p Доставка по Москве в пределах МКАД - 
									span 1 000 руб.
								p Доставка по&nbsp;Москве в&nbsp;пределах МКАД при заказе от&nbsp;10&nbsp;000&nbsp;руб.&nbsp;&mdash;
									span &nbsp;бесплатно
							+e.calculate.btn.btn-outline-primary(href="#")
								| Расчитать<br>стоимость доставки
								+icon('arrow-right')
							+e.payment
								h6 Доставка
								p
									+icon('money')
									| Наличные
								p
									+icon('money')
									| Безналичные
					.row
						.col-12
							+e.advantages-row.row
								.col-6.col-md-auto
									+e.item
										+icon('refund')
										| Гарантия<br>возврата
								.col-6.col-md-auto
									+e.item
										+icon('track')
										| Круглосуточная<br>доставка по всей России
								.col-6.col-md-auto
									+e.item
										+icon('award')
										| Товар<br>сертифицирован
								.col-6.col-md-auto
									+e.item
										//- +icon('wallet')
										+icon('award')
										| Возможность<br>отсрочки платежа
			+e.footer-btn-row.row
				.col-6.col-md-auto
					+e.footer-btn.btn(href="#") Характеристики
				.col-6.col-md-auto
					+e.footer-btn.btn(href="#") Описание
				.col-6.col-md-auto
					+e.footer-btn.btn(href="#") Отзывы
				.col-6.col-md-auto
					+e.footer-btn.btn(href="#") Сертификат соответствия
	// end sProdCard